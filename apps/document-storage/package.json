        "morgan": "^1.10.0",
        "multer": "^1.4.1",
        "pino": "7.11.0",
        "pino-http": "7.0.0",
        "pino-pretty": "^7.6.1",
        "swagger-ui-express": "^4.3.0"
    },
    "devDependencies": {
        "@types/jest": "^29.4.0",
        "cross-env": "^7.0.3",
        "eslint": "8.14.0",
