{
    "name": "@pins/document-storage",
    "version": "0.0.0",
    "private": true,
    "description": "Sends documents to and retrieves documents from Azure Blob Storage",
    "engines": {
        "node": ">=16.14.0"
    },
    "type": "module",
    "scripts": {
        "dev": "nodemon -e js ./src/server.js",
        "test": "NODE_ENV=test cross-env NODE_OPTIONS=--experimental-vm-modules npx jest",
