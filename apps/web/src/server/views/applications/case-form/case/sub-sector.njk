{% from "govuk/components/radios/macro.njk" import govukRadios %}

{% macro subSector(values, errors, subSectors) %}
	{{ govukRadios({
		name: "subSectorName",
		idPrefix: "subSectorName",
		errorMessage: errors.subSectorName | errorMessage,
		items: subSectors | selectItems({valueKey: 'name', labelKey: 'displayNameEn', selectedValue: values.subSectorName})
	}) }}
{% endmacro %}
