{
  "name": "web",
  "version": "0.0.0",
  "private": true,
  "description": "PINS Back Office Web App",
  "engines": {
    "node": ">=16.14.0"
  },
  "scripts": {
    "dev": "npm-run-all --silent rollup sass --parallel --race watch:** nodemon",
    "start": "node ./src/server/server.js",
    "build": "echo 'TOOD: add build script'",
    "nodemon": "nodemon --config ./nodemon.json ./src/server/server.js",
    "rollup": "node ./scripts/bundle-js.js",
    "sass": "node ./scripts/compile-css.js",
    "watch:rollup": "chokidar \"src/client/**/*.js\" --silent -c \"npm run rollup\"",
    "watch:sass": "chokidar \"src/styles/**/*.scss\" --silent -c \"npm run sass\"",
    "test": "echo 'TOOD: add test script'",
    "lint:js": "eslint src",
    "lint:js:fix": "eslint src --fix",
    "clean": "rimraf .turbo node_modules dist"
  },
  "dependencies": {
    "body-parser": "1.19.2",
    "compression": "1.7.4",
    "cookie-parser": "1.4.6",
    "cors": "2.8.5",
    "dotenv": "16.0.0",
    "express": "4.17.3",
    "figures": "3.2.0",
    "govuk-frontend": "4.0.1",
    "helmet": "5.0.2",
    "kleur": "4.1.4",
    "lodash": "^4.17.21",
    "morgan": "1.10.0",
    "nunjucks": "3.2.3",
    "planning-inspectorate-libs": "*",
    "serve-static": "^1.14.2",
    "signale": "1.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.17.5",
    "@babel/preset-env": "^7.16.11",
    "@rollup/plugin-alias": "^3.1.9",
    "@rollup/plugin-babel": "^5.3.1",
    "@rollup/plugin-beep": "^0.2.0",
    "@rollup/plugin-commonjs": "^21.0.2",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@rollup/plugin-replace": "^4.0.0",
    "@rollup/plugin-virtual": "^2.1.0",
    "autoprefixer": "10.4.2",
    "chokidar-cli": "^3.0.0",
    "core-js": "^3.21.1",
    "eslint": "8.10.0",
    "eslint-config": "*",
    "node-notifier": "10.0.1",
    "nodemon": "2.0.15",
    "npm-run-all": "^4.1.5",
    "postcss": "8.4.7",
    "rimraf": "^3.0.2",
    "rollup": "2.69.0",
    "rollup-plugin-size": "^0.2.2",
    "rollup-plugin-visualizer": "^5.6.0",
    "sass": "1.49.9",
    "terser": "^5.12.0"
  }
}
