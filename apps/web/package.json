    "@rollup/plugin-node-resolve": "*",
    "@rollup/plugin-replace": "*",
    "@rollup/plugin-virtual": "*",
    "@testing-library/dom": "*",
    "@testing-library/jest-dom": "*",
    "@testing-library/user-event": "*",
    "@types/compression": "*",
    "@types/cookie-parser": "*",
