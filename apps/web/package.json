{
  "name": "@pins/applications.web",
  "version": "0.0.0",
  "private": true,
  "description": "PINS Back Office Web App",
  "engines": {
    "node": ">=20.0.0 <21.0.0"
  },
  "type": "module",
  "scripts": {
    "build": "cross-env NODE_ENV=production npm-run-all --silent sass rollup",
    "build:release": "cross-env NODE_ENV=production APP_RELEASE=true npm-run-all --silent sass rollup",
    "clean": "rimraf .turbo node_modules dist",
    "clean:dev": "rimraf dist src/server/static/scripts src/server/static/styles",
    "dev": "cross-env NODE_ENV=local npm-run-all --silent clean:dev sass rollup --parallel --race watch:** nodemon",
    "lint:js": "npx eslint .",
    "lint:js:fix": "npx eslint . --fix",
    "nodemon": "nodemon --config ./nodemon.json ./src/server/server.js",
    "rollup": "node ./scripts/rollup/bundle-js.js",
    "sass": "node ./scripts/rollup/compile-css.js",
    "start": "node ./src/server/server.js",
    "test": "cross-env NODE_ENV=test NODE_OPTIONS=\"--experimental-vm-modules --max-old-space-size=8192\" npx jest  --coverage",
    "test:cov": "cross-env NODE_ENV=test NODE_OPTIONS=--experimental-vm-modules npx jest --coverage",
    "tscheck": "npx tsc -p jsconfig.json --maxNodeModuleJsDepth 0",
    "watch:rollup": "chokidar \"src/client/**/*.js\" --silent -c \"npm run rollup\"",
    "watch:sass": "chokidar \"src/styles/**/*.scss\" --silent -c \"npm run sass\""
  },
  "dependencies": {
    "@azure/msal-node": "*",
    "@azure/storage-blob": "*",
    "@pins/add-auth-headers-for-backend": "*",
    "@pins/applications": "*",
    "@pins/blob-storage-client": "*",
    "@pins/platform": "*",
    "@pins/redis": "*",
    "@pins/examination-timetable-utils": "*",
    "@planning-inspectorate/address-lookup": "*",
    "@toast-ui/editor": "*",
    "accessible-autocomplete": "*",
    "applicationinsights": "*",
    "body-parser": "*",
    "classnames": "*",
    "compression": "*",
    "cookie-parser": "*",
    "cors": "*",
    "date-fns": "*",
    "date-fns-tz": "*",
    "express": "*",
    "express-request-id": "*",
    "express-session": "*",
    "express-validator": "*",
    "got": "*",
    "govuk-frontend": "*",
    "helmet": "*",
    "humps": "*",
    "joi": "*",
    "kleur": "*",
    "lodash-es": "*",
    "node-cache": "*",
    "nunjucks": "*",
    "pino": "*",
    "pino-pretty": "*",
    "pluralize": "*",
    "redis": "*",
    "response-time": "*",
    "sanitize-html": "*",
    "serve-static": "*",
    "slugify": "*"
