{
	"$schema": "http://json-schema.org/draft-07/schema#",
	"$id": "interested-party.schema.json",
	"title": "interested-party",
	"description": "Subset of Pins Data Model [Service User]",
	"type": "object",
	"required": ["id"],
	"properties": {
		"id": {
			"type": "number",
			"examples": [18]
		},
		"interestedPartyNumber": {
			"type": "string",
			"examples": ["IP1234"]
		},
		"firstName": {
			"type": "string",
			"examples": ["Alan"]
		},
		"lastName": {
			"type": "string",
			"examples": ["Turing"]
		},
		"under18": {
			"type": "boolean"
		},
		"organisationName": {
			"title": "Name",
			"type": "string",
			"default": "",
			"examples": ["National Highways"],
			"pattern": "^.*$"
		},
		"contactMethod": {
			"type": "string",
			"enum": ["email", "post"]
		},
		"emailAddress": {
			"type": "string",
			"examples": ["alan.turing@planninginspectorate.gov.uk"]
		},
		"telephone": {
			"type": "string",
			"examples": ["0137732432"]
		},
		"address": {
			"title": "Address",
			"type": "object",
			"required": ["addressLine1", "town", "postcode"],
			"properties": {
				"addressLine1": {
					"type": "string",
					"examples": ["National Traffic Operations Centre"]
				},
				"addressLine2": {
					"type": "string",
					"examples": ["3 Ridgeway, Quinton Business Park"]
				},
				"town": {
					"type": "string",
					"examples": ["Birmingham"]
				},
				"postcode": {
					"type": "string",
					"examples": ["B32 1AF"]
				}
			}
		}
	}
}
