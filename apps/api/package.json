{
  "name": "@pins/applications.api",
  "version": "0.0.0",
  "private": true,
  "description": "PINS Back Office API App",
  "engines": {
    "node": ">=18.0.0 <19.0.0"
  },
  "prisma": {
    "schema": "src/database/schema.prisma",
    "seed": "node src/database/seed/seed-development.js"
  },
  "type": "module",
  "scripts": {
    "test": "npm run prisma-generate && cross-env NODE_ENV=test NODE_OPTIONS=--experimental-vm-modules npx jest --detectOpenHandles --forceExit",
    "test:watch": "npm run test -- --watch",
    "coverage": "npm run prisma-generate && cross-env NODE_ENV=test NODE_OPTIONS=--experimental-vm-modules npx jest --coverage",
    "dev": "npm run prisma-generate && nodemon --experimental-json-modules -e js ./src/server.js",
    "start": "node ./src/server.js",
    "lint:js": "eslint .",
    "lint:js:fix": "eslint . --fix",
    "clean": "rimraf .turbo node_modules dist",
    "gen-api-spec": "node ./src/server/gen-api-spec.js",
    "db:migrate:generate": "npx prisma migrate dev --create-only",
    "db:migrate": "npx prisma migrate dev",
    "db:migrate:prod": "npx prisma migrate deploy",
    "db:push": "npx prisma db push --accept-data-loss",
    "db:reset": "npx prisma migrate reset --force",
    "db:seed": "npm run prisma-generate && npx prisma db seed",
    "db:seed:prod": "npm run prisma-generate && cross-env NODE_ENV=production node src/database/seed/seed-production.js",
    "prisma:format": "npx prisma format",
    "prisma-generate": "npx prisma generate",
    "tscheck": "npx tsc -p jsconfig.json --maxNodeModuleJsDepth 0"
  },
  "dependencies": {
    "@azure/identity": "3.0.1",
    "@azure/monitor-opentelemetry-exporter": "^1.0.0-beta.18",
    "@azure/service-bus": "^7.7.3",
    "@opentelemetry/instrumentation": "^0.45.1",
    "@opentelemetry/resources": "^1.18.1",
    "@opentelemetry/sdk-trace-base": "^1.18.1",
    "@opentelemetry/sdk-trace-node": "^1.18.1",
    "@opentelemetry/semantic-conventions": "^1.18.1",
    "@pins/applications": "^0.0.0",
    "@pins/event-client": "^0.0.0",
    "@pins/express": "^0.0.0",
    "@pins/key-vault-secrets-client": "^0.0.0",
    "@pins/platform": "^0.0.0",
    "@prisma/client": "^4.16.1",
    "@prisma/instrumentation": "^5.6.0",
    "@supercharge/promise-pool": "^2.4.0",
    "ajv": "^8.12.0",
    "ajv-formats": "^2.1.1",
    "applicationinsights": "^2.9.1",
    "body-parser": "1.20.0",
    "c8": "^7.11.3",
    "compression": "1.7.4",
    "cors": "2.8.5",
    "csv-stringify": "^6.4.0",
    "date-fns": "2.28.0",
    "dotenv": "16.0.3",
    "express": "4.18.1",
    "express-pino-logger": "^7.0.0",
    "express-validator": "6.14.2",
    "got": "^12.5.2",
    "helmet": "5.1.0",
    "joi": "^17.7.0",
    "lodash-es": "^4.17.21",
    "morgan": "1.10.0",
    "node-cache": "^5.1.2",
    "node-fetch": "^3.3.1",
    "pino": "^8.1.0",
    "pino-http": "^8.1.0",
    "pino-pretty": "^8.1.0",
    "pins-data-model": "github:Planning-Inspectorate/data-model#draft-05",
    "rhea": "3.0.1",
    "sanitize-html": "^2.12.1",
    "swagger-ui-express": "^4.4.0",
    "xstate": "4.32.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/express-pino-logger": "^4.0.3",
    "@types/jest": "^29.4.0",
    "cross-env": "^7.0.3",
    "eslint": "8.18.0",
    "jest": "^29.4.2",
    "jest-environment-jsdom": "^29.3.1",
    "jest-mock-extended": "^3.0.1",
    "nodemon": "^3.0.3",
    "prisma": "4.16.1",
    "rimraf": "^3.0.2",
    "supertest": "6.2.3",
    "swagger-autogen": "^2.21.4",
    "swagger-typescript-api": "^13.0.3"
  },
  "imports": {
    "#app-test": "./src/server/app-test.js",
    "#config/*": "./src/server/config/*",
    "#infrastructure/*": "./src/server/infrastructure/*",
    "#middleware/*": "./src/server/middleware/*",
    "#repositories/*": "./src/server/repositories/*",
    "#utils/*": "./src/server/utils/*",
    "#message-schemas/*": "./src/message-schemas/*"
  }
}
