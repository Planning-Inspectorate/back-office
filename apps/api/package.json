{
  "name": "@pins/applications.api",
  "version": "0.0.0",
  "private": true,
  "description": "PINS Back Office API App",
  "engines": {
    "node": ">=20.0.0 <21.0.0"
  },
  "prisma": {
    "schema": "src/database/schema.prisma",
    "seed": "node src/database/seed/seed-development.js"
  },
  "type": "module",
  "scripts": {
    "test": "npm run prisma-generate && cross-env NODE_ENV=test NODE_OPTIONS=--experimental-vm-modules npx jest --forceExit --coverage",
    "test:nocov": "npm run prisma-generate && cross-env NODE_ENV=test NODE_OPTIONS=--experimental-vm-modules npx jest --forceExit",
    "test:watch": "npm run test -- --watch",
    "coverage": "npm run prisma-generate && cross-env NODE_ENV=test NODE_OPTIONS=--experimental-vm-modules npx jest --forceExit --coverage",
    "dev": "cross-env TZ=utc npm run prisma-generate && nodemon --experimental-json-modules -e js ./src/server.js",
    "dev:debug": "cross-env TZ=utc npm run prisma-generate && nodemon --inspect --experimental-json-modules -e js ./src/server.js",
    "start": "node ./src/server.js",
    "lint:js": "eslint .",
    "lint:js:fix": "eslint . --fix",
    "clean": "rimraf .turbo node_modules dist",
    "gen-api-spec": "node ./src/server/gen-api-spec.js",
    "db:migrate:generate": "npx prisma migrate dev --create-only",
    "db:migrate": "npx prisma migrate dev",
    "db:migrate:prod": "npx prisma migrate deploy",
    "db:push": "npx prisma db push --accept-data-loss",
    "db:reset": "npx prisma migrate reset --force",
    "db:seed": "npm run prisma-generate && npx prisma db seed",
    "db:seed:prod": "npm run prisma-generate && cross-env NODE_ENV=production node src/database/seed/seed-production.js",
    "db:create:gs51": "npm run prisma-generate && node src/database/seed/seed-gs51.js",
    "prisma:format": "npx prisma format",
    "prisma-generate": "npx prisma generate",
    "tscheck": "npx tsc -p jsconfig.json --maxNodeModuleJsDepth 0"
  },
  "dependencies": {
    "@azure/monitor-opentelemetry-exporter": "*",
    "@opentelemetry/instrumentation": "*",
    "@opentelemetry/resources": "*",
    "@opentelemetry/sdk-trace-base": "*",
    "@opentelemetry/sdk-trace-node": "*",
    "@opentelemetry/semantic-conventions": "*",
    "@pins/applications": "*",
    "@pins/event-client": "*",
    "@pins/express": "*",
    "@pins/key-vault-secrets-client": "*",
    "@pins/examination-timetable-utils": "*",
    "@pins/platform": "*",
    "@prisma/instrumentation": "*",
    "@prisma/client": "*",
    "@supercharge/promise-pool": "*",
    "ajv": "*",
    "ajv-formats": "*",
    "applicationinsights": "*",
    "body-parser": "*",
    "compression": "*",
    "csv-stringify": "*",
    "date-fns": "*",
    "date-fns-tz": "*",
    "dotenv": "*",
    "express": "*",
    "express-pino-logger": "*",
    "express-validator": "*",
    "helmet": "*",
    "joi": "*",
    "lodash-es": "*",
    "node-cache": "*",
    "p-map": "*",
    "pino": "*",
    "pino-http": "*",
    "pino-pretty": "*",
	"@planning-inspectorate/data-model": "*",
    "sanitize-html": "*",
    "swagger-ui-express": "*",
    "xstate": "*"
  },
  "devDependencies": {
    "@types/express": "*",
    "@types/express-pino-logger": "*",
    "@types/jest": "*",
    "chalk": "*",
    "cross-env": "*",
    "eslint": "*",
    "jest": "*",
    "nodemon": "*",
    "prisma": "*",
    "rimraf": "*",
    "supertest": "*",
    "swagger-autogen": "*",
    "swagger-typescript-api": "*"
  },
  "imports": {
    "#app-test": "./src/server/app-test.js",
		"#api-constants": "./src/server/applications/constants.js",
    "#config/*": "./src/server/config/*",
    "#infrastructure/*": "./src/server/infrastructure/*",
    "#middleware/*": "./src/server/middleware/*",
    "#repositories/*": "./src/server/repositories/*",
    "#utils/*": "./src/server/utils/*",
    "#message-schemas/*": "./src/message-schemas/*"
  }
}
