{
	"version": "2.0",
	"logging": {
		"applicationInsights": {
			"samplingSettings": {
				"isEnabled": true,
				"excludedTypes": "Request"
			}
		},
		"logLevel": {
			"default": "Information",
			"Host": "Information",
			"Function": "Debug",
			"Worker": "Debug",
			"Microsoft": "Warning"
		}
	},
	"extensionBundle": {
		"id": "Microsoft.Azure.Functions.ExtensionBundle",
		"version": "[3.*, 4.0.0)"
	},
	"functionTimeout": "00:30:00"
}
