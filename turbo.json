{
	"npmClient": "npm",
	"pipeline": {
		"build": {
			"outputs": [
				"src/server/static/scripts/**",
				"src/server/static/styles/**"
			],
			"dependsOn": [
				"^build",
				"$NODE_ENV",
				"$APP_RELEASE"
			]
		},
		"build:release": {
			"outputs": [
				"src/server/static/scripts/**",
				"src/server/static/styles/**"
			],
			"dependsOn": [
				"^build",
				"$NODE_ENV",
				"$APP_RELEASE"
			]
		},
		"test": {
			"outputs": [
				"coverage/**"
			],
			"dependsOn": []
		},
		"lint:js": {
			"dependsOn": [
				"^build"
			],
			"outputs": []
		},
		"lint:js:fix": {
			"dependsOn": [
				"^build"
			],
			"outputs": []
		},
		"dev": {
			"cache": false
		},
		"clean": {
			"cache": false
		}
	}
}
