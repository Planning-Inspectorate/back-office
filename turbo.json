{
	"npmClient": "npm",
	"pipeline": {
		"build": {
			"outputs": ["src/server/static/scripts/**", "src/server/static/styles/**"],
			"dependsOn": ["^build", "$NODE_ENV", "$APP_RELEASE"]
		},
		"build:release": {
			"outputs": ["src/server/static/scripts/**", "src/server/static/styles/**"],
			"dependsOn": ["^build", "$NODE_ENV", "$APP_RELEASE"]
		},
		"topo": {
			"dependsOn": ["^topo"]
		},
		"test": {
			"outputs": ["coverage/**"],
			"dependsOn": ["topo"]
		},
		"lint:js": {
			"dependsOn": ["^build"],
			"outputs": []
		},
		"lint:js:fix": {
			"dependsOn": ["^build"],
			"outputs": []
		},
		"prisma-generate": {
			"cache": false
		},
		"tscheck": {
			"dependsOn": ["topo"]
		},
		"dev": {
			"cache": false
		},
		"clean": {
			"cache": false
		}
	}
}
